            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.jmeter.util.JMeterUtils;

log.info(&quot;Debugging counter according to SQS Batch Size&quot;);

def varCounterSQSInteger = vars.get(&quot;counterSQS&quot;).toInteger();
def varThreadCountInteger = vars.get(&quot;threadCount&quot;).toInteger();

log.info(&quot;!!! varCounterSQSInteger = {$varCounterSQSInteger}&quot;);
log.info(&quot;!!! varThreadCount = {$varThreadCountInteger}&quot;);
</stringProp>
