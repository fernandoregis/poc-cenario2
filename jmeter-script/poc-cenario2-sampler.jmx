          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="SQS-Dispatcher [JSR223 Sampler]" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.jmeter.util.JMeterUtils

log.info(&quot;Inicio: SQS Dispatcher&quot;)

String basePath = &quot;C:\\\\src\\\\aws-testF\\\\&quot;
String scriptBatName = &quot;test-sqs.bat&quot;
String scriptBatLogName = &quot;jmeter-test-output\\\\test-sqs-bat.log&quot;

def varCounterSQSObject = vars.get(&quot;counterSQS&quot;);
def varSqsBatchSizeObject = vars.get(&quot;sqsBatchSize&quot;)

def varCounterSQSInteger = varCounterSQSObject.toInteger();
def varSqsBatchSizeInteger = varSqsBatchSizeObject.toInteger();

def propLoopCountSQSInteger = props.get(&quot;loop.count&quot;).toInteger();

//if (varCounterSQSInteger &gt; propLoopCountSQSInteger) return;

def baseModuloExecucaoSQS = varCounterSQSInteger + (varSqsBatchSizeInteger-1);

def mustExecuteSQSBatch = baseModuloExecucaoSQS % varSqsBatchSizeInteger == 0;

if ( !mustExecuteSQSBatch ) {
	log.info(&quot;Nao Executa SQS&quot;);
	log.info(&quot;Fim: SQS Dispatcher&quot;)
	return;
}

log.info(&quot;Executando SQS Batch Send&quot;);

if (vars.get(&quot;debugFlow&quot;).toBoolean()) {
	log.info(&quot;DEBUGGING Flow&quot;);
	log.info(&quot;Fim: SQS Dispatcher&quot;)
	
	def waitForMiliseconds = new Random().nextInt(251) + 750;

	Thread.sleep(waitForMiliseconds);
	return; //Mocking / DEBUG
}else{
	log.info(&quot;REAL Flow ( USING AWS )&quot;);
}
	
//String command = &quot;cmd.exe /c ${basePath}${scriptBatName} | tee ${basePath}${scriptBatLogName} -Append&quot;
//String command = &quot;cmd.exe /c ${basePath}${scriptBatName} &gt;&gt; ${basePath}${scriptBatLogName}&quot;
String command = &quot;cmd.exe /c ${basePath}${scriptBatName}&quot;

def stdOut = new StringBuilder(), strErr = new StringBuilder()

log.info(&quot;Command Input: &quot; + command)

Process process = command.execute()

process.consumeProcessOutput(stdOut, strErr)
process.waitFor()

def output = process.text

log.info(&quot;Command Output: (&quot; + output + &quot;)&quot;)
log.info(&quot;Std Output: (&quot; + stdOut + &quot;)&quot;)
log.info(&quot;Str Err: (&quot; + strErr + &quot;)&quot;)

log.info(&quot;Fim: SQS Dispatcher&quot;)</stringProp>
